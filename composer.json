{
	"name" : "bjnstnkvc/builder-make-command",
	"description": "Generate Eloquent Builder class for enhanced query building and model scope management.",
	"authors": [
		{
			"name" : "Bojan Stankovic",
			"email": "dev.bojan.stankovic@gmail.com"
		}
	],
	"keywords": [
		"laravel",
		"eloquent",
		"builder",
		"query",
		"scope"
	],
	"type" : "package",
	"license": "MIT",
	"autoload" : {
		"psr-4": {
			"Bjnstnkvc\\BuilderMakeCommand\\": "src/"
		}
	},
	"autoload-dev" : {
		"psr-4": {
			"Bjnstnkvc\\BuilderMakeCommand\\Tests\\": "tests/",
			"App\\" : "workbench/app",
			"Database\\Factories\\" : "workbench/database/factories/",
			"Database\\Seeders\\" : "workbench/database/seeders/"
		}
	},
	"minimum-stability": "dev",
	"extra": {
		"laravel": {
			"providers": [
				"Bjnstnkvc\\BuilderMakeCommand\\BuilderMakeCommandServiceProvider"
			]
		}
	},
	"require": {
		"illuminate/console" : "^7.0|^8.0|^9.0|^10.0|^11.0",
		"illuminate/support" : "^7.0|^8.0|^9.0|^10.0|^11.0",
		"illuminate/database": "^7.0|^8.0|^9.0|^10.0|^11.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.0|^8.0|^9.0|^10.0|^11.0",
		"orchestra/testbench": "^7.0|^8.0|^9.0|^10.0|^11.0"
	}
}
